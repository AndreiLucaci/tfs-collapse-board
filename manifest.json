{
    "manifest_version": 2,
  
    "name": "TFS collapse board",
    "description": "This extension allows the user to collapse the TFS board",
    "version": "1.2.2.3",
  
    "permissions": [
        "activeTab"
      ],
      "background": {
        "scripts": ["background.js"]
      },

      "browser_action": {
        "default_icon": "icon128.png"
    },
    "icons": {
      "128": "icon128.png"
    }
  }